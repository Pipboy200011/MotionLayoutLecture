<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.mw2c.pipboy200011.motionlayoutlecture"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".other.OtherMotionExampleActivity">
        </activity>
        <activity android:name=".viewpagerwithmotion.ViewPagerWithMotionExampleActivity">
        </activity>
        <activity
            android:name=".collapsingwithmotion.CollapsingWithMotionExampleActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        </activity>
        <activity android:name=".keyframes.KeyFramesExampleActivity"/>
        <activity android:name=".customattribute.CustomAttributeExampleSecondActivity"/>
        <activity android:name=".customattribute.CustomAttributeExampleFirstActivity"/>
        <activity android:name=".motionscene.MotionSceneExampleSecondActivity"/>
        <activity android:name=".motionscene.MotionSceneExampleFirstActivity"/>
        <activity android:name=".main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>